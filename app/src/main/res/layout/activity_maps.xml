<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"

    >



<fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/map"
    tools:context="com.example.chuanzhu.myapplication.MapsActivity"
    android:name="com.google.android.gms.maps.SupportMapFragment"

    android:layout_above="@+id/editText2"
    android:layout_below="@+id/button5"
    android:layout_marginTop="10dp"
    />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/button_report"
        android:id="@+id/button9"
        android:textStyle="bold"
        android:textColor="#f5ad3f"
        android:textSize="20dp"
        android:onClick="reportcrime"
        android:background="#000000"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

  <!--  <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="@string/button_filter"
        android:textStyle="bold"
        android:textColor="#f5ad3f"
        android:id="@+id/button8"
        android:textSize="20dp"
        android:onClick="filtercrime"
        android:background="#000000"
        android:layout_above="@+id/button9"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />-->

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPostalAddress"
        android:ems="10"
        android:id="@+id/editText2"
        android:background="#000000"
        android:textColor="#13aeef"


        android:layout_alignTop="@+id/textView6"
        android:layout_toLeftOf="@+id/go"
        android:layout_toStartOf="@+id/go" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Location"
        android:id="@+id/textView6"
        android:textColor="#f5ad3f"
        android:textSize="15dp"
        android:textStyle="bold"
        android:layout_marginBottom="5dp"
        android:layout_above="@+id/button9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="Go"
        android:id="@+id/go"
        android:textStyle="bold"
        android:background="#000000"
        android:textColor="#f5ad3f"
        android:onClick="Geogo"
        android:layout_alignTop="@+id/editText2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logout"
        android:id="@+id/button5"
        android:textStyle="bold"
        android:background="#000000"
        android:textColor="#f5ad3f"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="gomain"

        />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profile"
        android:id="@+id/button6"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/button5"
        android:layout_toStartOf="@+id/button5"
        android:textStyle="bold"
        android:background="#000000"
        android:textColor="#f5ad3f"
        android:layout_marginRight="5dp"
        android:onClick="goprofile"
        />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Setting"
        android:id="@+id/button10"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/button6"
        android:layout_toStartOf="@+id/button6"
        android:textStyle="bold"
        android:background="#000000"
        android:textColor="#f5ad3f"
        android:layout_marginRight="5dp"
        android:onClick="gosetting"
        />

    <ImageView
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:id="@+id/imageView3"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@mipmap/ic_launcher2"
        android:layout_alignBottom="@+id/button10"
        android:layout_toStartOf="@+id/editText2"
        android:layout_toLeftOf="@+id/editText2" />


</RelativeLayout>

